<% content_for :ctas_secondary do %>
  <%= link_to app_queues_path, class: "btn-secondary-gray flex-none" do %>
    <%= icon('fa-solid','arrow-left', class:'-mx-1')%>
  <%end%>

  <%= link_to edit_app_queue_path(@app_queue), class: "btn-secondary flex-none" do %>
    <%= icon('fa-solid','pen-to-square', class:'-mx-1')%>
  <%end%>

  <%= button_to app_queue_path(@app_queue), method: :delete, data: { turbo: false },onclick: "return confirm('#{t('views.app.general.delete_confirm')}')",class: "btn-delete flex-none" do %>
    <%= icon('fa-solid','trash-can', class:'-mx-1')%>
  <%end%>
<% end %>




<% provide :head_title, "#{t('views.app.general.showing')} #{App::Queue.model_name.human} " %>
<% provide :page_title, "#{t('views.app.general.showing')} #{App::Queue.model_name.human} " %>
<% provide :entry_id, "#{@app_queue.id}" %>

<% content_for :page_content do %>
  <%= render "detail", app_queue: @app_queue %>
<%end%>
