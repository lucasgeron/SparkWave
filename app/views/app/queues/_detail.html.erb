<div class="space-y-4">

  <div class="">
    <p class=""><%= App::HealthUnit.model_name.human%></p>
    <p class="detail-input"><%= app_queue.health_unit.name  %></p>
  </div>

  <% %i[category].each do |attr| %>
    <div class="">
      <p class=""><%= App::Queue.human_attribute_name(attr)%></p>
      <p class="detail-input"><%= app_queue[attr]  %></p>
    </div>
  <%end%>

  <% if app_queue.users.any? %>
    <div class="">
      <p class="font-bold text-2xl"> <%= App::User.model_name.human %> </p>
      <div class="mt-2 default-grid">
        <%= render app_queue.users %>
      </div>
    </div>
  <% end %>

</div>
